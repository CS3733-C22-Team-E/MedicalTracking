          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Medical Tracker" GridPane.columnIndex="1" GridPane.halignment="CENTER">
               <font>
